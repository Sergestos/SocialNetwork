@model IEnumerable<SocialNetwork.PresentationLayer.Models.UserView>

@{
    ViewBag.Title = "BlockedUsers";
}

<div class="bloked-users">
    @if (Model.Count() == 0)
    {
        <p style="font-size: 16px;">You have no users in the black list. Click <a href="~/User/Search">here</a> and find your victims!</p>
    }
    else
    {   
        @Helper.DisplayListOfUsers("Black list", "/User/BlockedUsers", Model, ViewBag.Page ?? 0, ViewBag.IsLastPage ?? false)
    }
</div>

